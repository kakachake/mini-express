{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "declaration": true,
    "outDir": "./lib",
    "baseUrl": "./src",
    "noEmit": false
  },
  "include": ["./src/**/*"]
}
